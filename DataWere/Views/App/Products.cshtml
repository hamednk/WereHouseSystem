        <thead>
            <tr>
                <th>نام</th>
                <th>تعداد</th>
                <th>توضیحات</th>
                <th>واحد</th>
                <th>حداقل موجودی</th>
                <th>حداقل موجودی</th>
                <th>عملیات</th>
            </tr>
        </thead>
        <tbody></tbody>
