@model IEnumerable<DataWere.Models.tblClient>
<div class="card-box table-responsive">
    <h4 class="m-t-0 header-title"><b>لیست مشتریان</b></h4>
    <div class="text-muted font-13 m-b-30">
        <button type="button" class="btn btn-success btn-rounded waves-effect waves-light pull-right" data-url="CreateClient" data-form="SaveClientForm" data-action="مشتری" onclick="CreateElement(this)">
            <span class="btn-label"><i class="fa fa-plus"></i></span>
            تعریف مشتری جدید
        </button>
    </div>
    <br />
    <br />
    <br />
    <table id="ClientsTbl" class="table table-striped table-bordered dataTable no-footer dtr-inline">
        <thead>
            <tr>                
                <th>نام</th>
                <th>نام مستعار</th>
                <th>شرکت</th>
                <th>NIPT</th>
                <th>تلفن</th>
                <th>ایمیل</th>
                <th>عملیات</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>
